<container xmlns="http://symfony.com/schema/dic/services"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

	<services>
		<service id="craueFormFlowDemoBundle.form.choicelist.topicCategory"
				class="Craue\FormFlowDemoBundle\Form\ChoiceList\TopicCategoryChoiceList">
			<call method="setTranslator">
				<argument type="service" id="translator" />
			</call>
		</service>

		<service id="craueFormFlowDemoBundle.form.choicelist.vehicleEngine"
				class="Craue\FormFlowDemoBundle\Form\ChoiceList\VehicleEngineChoiceList">
			<call method="setTranslator">
				<argument type="service" id="translator" />
			</call>
		</service>
	</services>
</container>
